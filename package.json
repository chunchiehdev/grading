{
  "name": "gradsystem",
  "version": "1.57.1",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "dev": "react-router dev",
    "dev:clean": "docker compose -f docker-compose.dev.yaml exec app rm -rf /app/node_modules/.vite && docker compose -f docker-compose.dev.yaml restart app",
    "dev:clean:full": "docker compose -f docker-compose.dev.yaml down && docker volume rm grading_app_node_modules 2>/dev/null || true && docker compose -f docker-compose.dev.yaml up -d",
    "dev:check": "bash scripts/check-vite-status.sh",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "lint:fix": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint . --ext .ts,.tsx --report-unused-disable-directives --fix",
    "start": "react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:safari": "npx playwright test --headed --project=webkit-desktop test/safari-visual.spec.ts",
    "test:safari:mobile": "npx playwright test --headed --project=webkit-mobile test/safari-visual.spec.ts",
    "migrate:dev": "npx prisma migrate dev && npx prisma generate",
    "migrate:prod": "npx prisma migrate deploy && npx prisma generate",
    "seed:admin": "tsx scripts/seed-first-admin.ts",
    "cleanup:jobs": "tsx scripts/cleanup-bullmq-jobs.ts",
    "test:rate-limits": "bash scripts/run-rate-limit-test.sh",
    "version:patch": "npm version patch --no-git-tag-version",
    "version:major": "npm version major --no-git-tag-version",
    "version:minor": "npm version minor --no-git-tag-version",
    "version:get": "node -p \"require('./package.json').version\"",
    "version:refresh": "node -e \"const fs=require('fs');const{execSync}=require('child_process');const pkg=require('./package.json');const branch=execSync('git rev-parse --abbrev-ref HEAD',{encoding:'utf8'}).trim();const commit=execSync('git rev-parse --short HEAD',{encoding:'utf8'}).trim();fs.writeFileSync('version.json',JSON.stringify({version:pkg.version,branch,commitHash:commit,buildTime:new Date().toISOString()},null,2))\""
  },
